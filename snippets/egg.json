{
  "config": {
    "prefix": "egg config",
    "body": [
      "'use strict';",
      "",
      "module.exports = appInfo => {",
      "  const config = {};",
      "",
      "  /**",
      "   * @member Config#$1",
      "   * @property {String} ${2:KEY} - ${3:description}",
      "   */",
      "  config.$1 = {",
      "    $0",
      "  };",
      "",
      "  return config;",
      "};",
      ""
    ]
  },
  "plugin": {
    "prefix": "egg plugin",
    "body": [
      "exports.$1 = {",
      "  enable: true,",
      "  package: '$2',",
      "};",
      ""
    ]
  },
  "service": {
    "prefix": "egg service",
    "body": [
      "'use strict';",
      "",
      "const Service = require('egg').Service;",
      "",
      "class $1Service extends Service {",
      "  $2",
      "}",
      "",
      "module.exports = $1Service;",
      ""
    ]
  },
  "controller": {
    "prefix": "egg controller",
    "body": [
      "'use strict';",
      "",
      "const Controller = require('egg').Controller;",
      "",
      "class $1Controller extends Controller {",
      "  $2",
      "}",
      "",
      "module.exports = $1Controller;",
      ""
    ]
  },
}
